{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "src",
    "target": "es6",
    "module": "es6",
    "esModuleInterop": true,
    "strictNullChecks": true,
    "declaration": false,
    "jsx": "react",
    "lib": [
      "dom",
      "es2019"
    ],
    "moduleResolution": "node"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
